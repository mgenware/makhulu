{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark" : [
            true,
            "single"
        ],
        "member-access": [
            false
        ],
        "no-var-requires": false,
        "ordered-imports": false,
        "max-classes-per-file": false,
        "no-empty": false,
        "max-line-length" :false
    },
    "rulesDirectory": []
}
